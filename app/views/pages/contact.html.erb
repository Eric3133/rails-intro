<h1>contact us</h1>

<h2>who do you want to contact?</h2>
<form action="/contact" method="get">
  <label for="member">please tupe a name...</label>
  <input id= "member" name= "member" type="text"
  value="<%= params[:member]%>"
  placeholder="chalie">
  <input type="submit">
</form>

<ol>
  <% @members.each do |member| %>
    <li><%= member.capitalize %></li>
  <% end %>
</ol>
